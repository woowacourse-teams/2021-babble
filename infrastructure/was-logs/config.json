{
    "agent": {
        "metrics_collection_interval": 60,
        "run_as_user": "root"
    },
    "metrics": {
        "metrics_collected": {
            "disk": {
                "measurement": [
                    "used_percent",
                    "used",
                    "total"
                ],
                "metrics_collection_interval": 60,
                "resources": [
                    "*"
                ]
            },
            "mem": {
                "measurement": [
                    "mem_used_percent",
                    "mem_total",
                    "mem_used"
                ],
                "metrics_collection_interval": 60
            }
        }
    },
    "logs": {
        "logs_collected": {
            "files": {
                "collect_list": [
                    {
                        "file_path": "/mnt/test-was-logs/error/error.log",
                        "log_group_name": "babble-was-test-error",
                        "log_stream_name": "/mnt/test-was-logs/error/error.log"
                    },
                    {
                        "file_path": "/mnt/test-was-logs/warn/warn.log",
                        "log_group_name": "babble-was-test-warn",
                        "log_stream_name": "/mnt/test-was-logs/warn/warn.log"
                    },
                    {
                        "file_path": "/mnt/test-was-logs/info/info.log",
                        "log_group_name": "babble-was-test-info",
                        "log_stream_name": "/mnt/test-was-logs/info/info.log"
                    },
                    {
                        "file_path": "/mnt/test-was-logs/debug/debug.log",
                        "log_group_name": "babble-was-test-debug",
                        "log_stream_name": "/mnt/test-was-logs/debug/debug.log"
                    },
                    {
                        "file_path": "/mnt/test-was-logs/trace/trace.log",
                        "log_group_name": "babble-was-test-trace",
                        "log_stream_name": "/mnt/test-was-logs/trace/trace.log"
                    },
                    {
                        "file_path": "/mnt/test-was-logs/access/access.log",
                        "log_group_name": "babble-was-test-access",
                        "log_stream_name": "/mnt/test-was-logs/access/access.log"
                    },
                    {
                        "file_path": "/mnt/main-was-logs/error/error.log",
                        "log_group_name": "babble-was-main-error",
                        "log_stream_name": "/mnt/main-was-logs/error/error.log"
                    },
                    {
                        "file_path": "/mnt/main-was-logs/warn/warn.log",
                        "log_group_name": "babble-was-main-warn",
                        "log_stream_name": "/mnt/main-was-logs/warn/warn.log"
                    },
                    {
                        "file_path": "/mnt/main-was-logs/info/info.log",
                        "log_group_name": "babble-was-main-info",
                        "log_stream_name": "/mnt/main-was-logs/info/info.log"
                    },
                    {
                        "file_path": "/mnt/main-was-logs/debug/debug.log",
                        "log_group_name": "babble-was-main-debug",
                        "log_stream_name": "/mnt/main-was-logs/debug/debug.log"
                    },
                    {
                        "file_path": "/mnt/main-was-logs/trace/trace.log",
                        "log_group_name": "babble-was-main-trace",
                        "log_stream_name": "/mnt/main-was-logs/trace/trace.log"
                    },
                    {
                        "file_path": "/mnt/main-was-logs/access/access.log",
                        "log_group_name": "babble-was-main-access",
                        "log_stream_name": "/mnt/main-was-logs/access/access.log"
                    }
                ]
            }
        }
    }
}
